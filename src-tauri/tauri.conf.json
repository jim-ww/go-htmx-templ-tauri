{
  "$schema": "https://schema.tauri.app/config/2.0.0-rc",
  "productName": "test",
  "version": "0.1.0",
  "identifier": "com.example.test-app",
  "build": {
    "frontendDist": "../static/",
    "devUrl": "http://localhost:8000",
    "beforeDevCommand": "python -m http.server -d static/"
  },
  "app": {
    "windows": [
      {
        "title": "Test-app",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
